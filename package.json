{
  "name": "hyshare",
  "version": "0.13.0",
  "description": "Share galleries and files from Hydrus on the web",
  "author": "floogulinc",
  "license": "MIT",
  "homepage": "https://github.com/floogulinc/hyshare",
  "repository": "floogulinc/hyshare",
  "bin": "./dist/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "tailwind:dev": "tailwindcss -i ./src/styles.css -o ./dist/assets/styles.css --watch",
    "tailwind:build": "tailwindcss -i ./src/styles.css -o ./dist/assets/styles.css --minify"
  },
  "files": [
    "/dist"
  ],
  "dependencies": {
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^9.4.3",
    "@nestjs/core": "^9.4.3",
    "@nestjs/platform-express": "^9.4.3",
    "@ruffle-rs/ruffle": "^0.1.0-nightly.2023.7.19",
    "byte-size": "^8.1.1",
    "cache-manager": "^3.6.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "content-disposition": "^0.5.4",
    "date-fns": "^2.30.0",
    "helmet": "^6.2.0",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "nest-typed-config": "2.4.3",
    "nunjucks": "^3.2.4",
    "on-headers": "^1.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.4.1",
    "rxjs": "^7.8.1",
    "utif": "^3.1.0",
    "zip-stream": "^4.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^9.4.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/archiver": "^5.3.2",
    "@types/byte-size": "^8.1.0",
    "@types/cache-manager": "^4.0.2",
    "@types/content-disposition": "^0.5.5",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.2",
    "@types/morgan": "^1.9.4",
    "@types/ms": "^0.7.31",
    "@types/node": "^20.6.0",
    "@types/nunjucks": "^3.2.2",
    "@types/on-headers": "^1.0.0",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.17",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "cross-env": "^7.0.3",
    "daisyui": "^2.52.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleDirectories": [
      "node_modules",
      "."
    ]
  }
}
